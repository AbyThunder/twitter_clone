<% @users.each do |user| %>
    <strong>Name</strong>: <%= user.name %></br>
    <strong>Handle</strong>: <%= user.handle %></br>
    <strong>Bio</strong>: <%= user.bio %></br>
    <strong>Email</strong>: <%= user.email %></br></br>
<% end %>

<%= button_to "Log out",  logout_path, method: :delete %>